.deg {
  position: relative;
  padding: 10rpx 10rpx;
  display: flex;
  align-items: center;
  line-height: 1.4;
  font-size: 14px;
}

.deg::after {
  position: absolute;
  left: 15px;
  right: 0;
  bottom: 0;
  border-top: 1rpx solid #e5e5e5;
  background: #e5e5e5;
  content: ' ';
}

.deg button {
  border: none;
  width: 100%;
  background: none;
  border-radius: 0;
  color: #333;
  font-size: 14px;
  text-align: left;
}

.deg button:after {
  border: none;
}

