.price-style {
  color: #e83d2c;
}

.shop-view {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
}

.shop-info-view {
  width: 686rpx;
  display: flex;
  flex-direction: row;
}

.shop-info-img {
  width: 144rpx;
  height: 144rpx;
  background-color: #000;
}

.shop-info-content {
  width: 518rpx;
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
}

.shop-content-name {
  font-size: 24rpx;
  color: #313131;
}

.shop-content-color {
  font-size: 24rpx;
  color: #919191;
}

.shop-price-number {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 30rpx;
}

.shop-price {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.shop-price-img {
  width: 24rpx;
  height: 26rpx;
}

.shop-price-text {
  font-size: 24rpx;
  color: #616161;
  margin-left: 5rpx;
}

.shop-number {
  font-size: 24rpx;
  color: #e83d2c;
}

.total-panel {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.total-view {
  width: 686rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.total-text {
  font-size: 24rpx;
  color: #313131;
}

.total-right {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.total-img {
  width: 24rpx;
  height: 26rpx;
}

.total-price {
  color: #616161;
  font-size: 24rpx;
  margin-left: 10rpx;
}

.box-cart-img {
  width: 30rpx;
  height: 32rpx;
}

/* 弹窗 */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.paymentModal {
  width: 100%;
  height: 858rpx;
  background: rgba(251, 251, 251, 1);
  border-radius: 36rpx 36rpx 0rpx 0rpx;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  z-index: 99999;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.confirmChange {
  font-size: 32rpx;
  color: #313131;
  margin-top: 24rpx;
}

.confirm-view {
  width: 702rpx;
  height: 2rpx;
  background-color: #c1c1c1;
  margin-top: 24rpx;
}

.change-number-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 60rpx;
}

.change-text {
  font-size: 64rpx;
  color: #616161;
  margin-left: 10rpx;
}

.change-account-view {
  width: 702rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.account-text {
  font-size: 24rpx;
  color: #919191;
}

.account-name {
  color: #313131;
  font-size: 24rpx;
}

.confirm-footer {
  width: 100%;
  height: 114rpx;
  background-color: #e5e5e5;
  position: fixed;
  bottom: 0;
  text-align: center;
  font-size: 32rpx;
  color: #e83d2c;
  line-height: 114rpx;
}

.pwdModal {
  width: 100%;
  height: 434rpx;
  background: #fff;
  border-radius: 36rpx 36rpx 36rpx 36rpx;
  overflow: hidden;
  position: fixed;
  top: 30%;
  z-index: 99999;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.forgetPwd{
  font-size: 24rpx;
  color: #919191;
  margin-top: 64rpx;
}